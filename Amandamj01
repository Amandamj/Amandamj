<script src="https://lib.baomitu.com/jquery/1.10.2/jquery.min.js"></script>
<script src="https://lib.baomitu.com/Swiper/2.7.0/idangerous.swiper.min.js"></script>
<script src="js/main.js?v=2"></script>
<script src="js/index_data.js"></script>
<script>
    $('.js-roadmap-btn').click(function () {
        var h = $('.roadmap-main')[0].scrollHeight;
        $('.roadmap-main').height(h);
        $(this).hide();
    });
    commonFooter();
    commonHeader_jp();
    indexCommonPopArticle();
    window.onload = function () {
        var mySwiper = new Swiper('.swiper-container', {
            pagination: '.pagination',
            loop: true,
            // grabCursor: true,
            // paginationClickable: true,
            preventClicks: false
        })
        $('.index-content2 .team .team-main .slide').css('display', 'flex');
        $('.arrow-left').on('click', function (e) {
            console.log(234)
            e.preventDefault()
            mySwiper.swipePrev()
        })
        $('.arrow-right').on('click', function (e) {
            e.preventDefault()
            mySwiper.swipeNext()
        })
    }

    $('.js-m-index-nav-x').click(function () {
        $('.m-index-nav-content').hide();
    });
    $('.js-m-index-nav-show').click(function () {
        $('.m-index-nav-content').show();
    });
    $('.js-index-watch').click(function () {
        $('.index-video-box').show();
    });
    $('.index-video-box .cover-bg').click(function () {
        $('.index-video-box').hide();
        $('.index-video')[0].pause();
    });
    // team 人物详情
    $('.js-index-team .item button').click(function () {
        var index = $(this).parent().index();
        var name = $(this).siblings('.name').text();
        var post = $(this).siblings('.post').text();
        var src = $(this).siblings('img')[0].src;
        $('.js-index-pop-detail .name').html(name);
        $('.js-index-pop-detail .post').html(post);
        $('.js-index-pop-detail .head')[0].src = src;
        $('.js-index-pop-detail .content').html(indexTeamArr_jp[index].t);
        $('.js-index-cover-bg').show();
        $('.js-index-pop-detail').show();
    });
    function toPop(obj) {
        var index = $(obj).attr('num');
        var name = $(obj).siblings('.name').text();
        var post = $(obj).siblings('.post').text();
        var src = $(obj).siblings('img')[0].src;
        $('.js-index-pop-detail .name').html(name);
        $('.js-index-pop-detail .post').html(post);
        $('.js-index-pop-detail .head')[0].src = src;
        $('.js-index-pop-detail .content').html(indexTeamArr_jp[index].t);
        $('.js-index-cover-bg').show();
        $('.js-index-pop-detail').show();
    }

    $('.js-index-pop-detail .x').click(function () {
        $('.js-index-pop-detail').hide();
        $('.js-index-cover-bg').hide();
    });
    $('.js-index-fullscreen').click(function () {
        $(this).parent().hide();
        $('.js-index-ecological-picbox').width('1200px');
        $('.js-index-fullscreen-close').show();
    });
    $('.js-index-fullscreen-close').click(function () {
        $('.js-system1-content').show();
        $('.js-index-ecological-picbox').width('645px');
        $('.js-index-fullscreen-close').hide();
    });
    $('.js-m-index-fullscreen').click(function () {
        $('.js-index-ecological-picbox img').width('100%');
    });
    $('.js-m-index-introduction-btn').click(function () { $('.m-index-introduction-content').height($('.m-index-introduction-content')[0].scrollHeight);$(this).hide(); })
    $('.js-system1-readmore-btn').click(function () { $('.system1-content-p').height($('.system1-content-p')[0].scrollHeight); $(this).hide(); })
    $('.js-m-aAPP-readmore-btn').click(function () { $('.m-dAPP-content').height($('.m-dAPP-content')[0].scrollHeight); $(this).hide(); })
    $('.js-m-index-system2-readmore-btn').click(function () { $('.m-index-system2-content').height($('.m-index-system2-content')[0].scrollHeight); $(this).hide(); })
    $('.js-m-index-readmore-btn').click(function () { $('.m-index-system4-content').height($('.m-index-system4-content')[0].scrollHeight); $(this).hide(); })

    $('.js-m-index-lang-choose').html('日本語');
</script>
